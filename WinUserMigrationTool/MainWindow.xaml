<Window x:Class="WinUserMigrationTool.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WinUserMigrationTool"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="CopyTestButton" Content="Copy paste selected" HorizontalAlignment="Left" Margin="10,381,0,0" VerticalAlignment="Top" Height="43" Width="137" Click="CopyTestButton_Click"/>
        <ListBox x:Name="UserListBox" d:ItemsSource="{d:SampleData ItemCount=5}" Margin="217,108,355,112" SelectionMode="Multiple"/>
        <Button x:Name="PopulateButton" Content="Populate listview" HorizontalAlignment="Left" Margin="176,381,0,0" VerticalAlignment="Top" Height="43" Width="155" Click="PopulateButton_Click"/>

    </Grid>
</Window>
